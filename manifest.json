{
  "manifest_version": 2,

  "name": "Highlander Tab",
  "version": "0.0.1",
  "description": "This will disable tab reloadings",
  "homepage_url": "http://brandynbold.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "tabs"
  ]
}